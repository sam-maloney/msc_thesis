FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];


internalField   uniform 1.05; 


boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1.05;
    }
    outlet
    {
//        type            advective;
//        phi             phi;
//        rho	            rho;
//        fieldInf	      1.05;
//        lInf	          4;
//        value           uniform 1.05;
        type            zeroGradient;
    }
    left
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


