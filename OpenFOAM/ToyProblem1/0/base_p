FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 0.75; 


boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.75;
    }
    outlet
    {
        type            advective;
        phi             phi;
        rho	            rho;
        fieldInf	      2.37037037037037;
        lInf	          4;
        value           uniform 2.37037037037037;
//        type            zeroGradient;
    }
    left
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


